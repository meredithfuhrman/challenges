<h2> Title: <%= @question.title %></h2>

<p><%= @question.description %></p>


<% @answers.each do |answer| %>
  <ul>
    <li><%= answer.description %></li>
  </ul>
<% end %>

<%= link_to 'Post an Answer', new_question_answer_path %>
<br>
<%= link_to 'Edit Question', edit_question_path(@question) %>
